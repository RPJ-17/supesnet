extends /../layout.pug

append scripts
	link(href='/scss/main.scss' rel="stylesheet")

block vars
	- var currentUrl = '/page'
	- var title = '| The Flash Temporada 6'

block content
	section.specific-page
		div.image-container
			img(src="/img/series/the-flash-season-6.jpeg" class="main-img")
		div.info-container
			div.information
				p Sexta temporada de la serie enfocada en el velocista escarlata, <strong>The Flash</strong>. Despues de la perdida de su hija, Barry tendrá que afrontar las consecuencias de la inminente e inevitable crisis y preparar a los miembros de su equipo para una vida sin él.
				h5 Información técnica
				ul.technical-data
					li Episodios: 05
					li Calidad: 720p
					li Audio: Inglés
					li Subtitulos: Inglés / Español latino
					li Formato: mkv
				h5 Carpeta personalizada
				div.folder-container
					img(src="/img/folders/the-flash-season-6-folder.jpg" class="folder")
				h5 Enlaces de descarga
				div.link-container
					ul.download-links
						li 
							a(href="#" class="link") 01 - Into the void
						li
							a(href="#" class="link") 02 - A flash of the lightning
						li
							a(href="#" class="link") 03 - Dead man running
						li
							a(href="#" class="link") 04 - There will be blood
						li
							a(href="#" class="link") 05 - Kiss kiss breach breach   
						li
							a(href="#" class="link") Carpeta personalizada
					// p Contraseña: supesnet

